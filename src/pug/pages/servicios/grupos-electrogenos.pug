extends /config/template
append config
  -pageTitle = 'Grupos Electrógenos'
  -pageDescription = 'Realizamos reparación y mantenimiento de Grupos Electrógenos, asi como los tableros de transferencia aplicando la experiencia y calidad que nos caracteriza en Elgeza. También ofrecemos la instalación de grupos electrógenos en Trujillo.'
  -ogTitle = 'Grupos Electrógenos | Elgeza'
  -ogUrl = 'https://elgeza.com/servicios/grupos-electrogenos.html'
  -ogImageUrl = 'https://elgeza.com/assets/img/seo/electrogenos.jpg'
  -ogImageAlt = 'Grupos Electrógenos | Elgeza'

block main
  script(type="application/ld+json").
    {
      "@context": "http://schema.org",
      "@type": "BreadcrumbList",
      "itemListElement": [{
        "@type": "ListItem",
        "position": 1,
        "name": "Elgeza",
        "item": "https://elgeza.com/"
      },{
        "@type": "ListItem",
        "position": 2,
        "name": "Servicios",
        "item": "https://elgeza.com/servicios.html"
      },{
        "@type": "ListItem",
        "position": 3,
        "name": "Grupos Electrógenos",
        "item": "https://elgeza.com/servicios/grupos-electrogenos.html"
      }]
    }
  +bannerAltern('/assets/img/electrogenos-banner.jpg', 'Grupos')
  main.service
    .service-container
      h3.service__title Electrógenos
      p.service__description Realizamos reparación y mantenimiento de Grupos Electrógenos, asi como los tableros de transferencia aplicando la experiencia y calidad que nos caracteriza en Elgeza. También ofrecemos la instalación de grupos electrógenos en Trujillo.
    .service-images
      h3.service-images__title Mantenimiento y reparación de grupos electrógenos
      +imagesService('/assets/img/electrogenos-1.jpg', 'Grupos electrógenos', 'Grupos electrógenos')
      +imagesService('/assets/img/electrogenos-2.jpg', 'Grupos electrógenos', 'Grupos electrógenos')
      +imagesService('/assets/img/electrogenos-3.jpg', 'Grupos electrógenos', 'Grupos electrógenos')
      +imagesService('/assets/img/electrogenos-4.jpg', 'Grupos electrógenos', 'Grupos electrógenos')
  section.contact-us(style='--bg-image: url(/assets/img/electrogenos-contact.jpg);')
    .contact-us-container
      p.contact-us__description <span class="contact-us__description--big">Contáctanos ahora y empecemos a trabajar</span> <br><span>La confianza nos caracteriza para brindar este servicio<span>
      p.contact-us__numbers LLÁMANOS AL<br><span class="contact-us__numbers--big"><span>RPM: 998 006 273</span><span>RPC: 992 280 087</span></span>
      a.button.button--yellow(href="/contacto.html") Contáctanos Ahora
  include /includes/map
