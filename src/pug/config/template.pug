block config
  include variables
  include /components/card-service
  include /components/skill-item
  include /components/banner-altern
doctype html
html(lang="es")
  head
    title My awesome title
    meta(charset="utf-8")
    meta(name="viewport" content="initial-scale=1.0, width=device-width")
    meta(name="twitter:card" content="summary_large_image")
    meta(name="twitter:site" content=twitterAccount)
    meta(property="og:title" content=pageTitle)
    meta(property="og:description" content=pageDescription)
    meta(property="og:image" content=pageImage)
    link(rel="stylesheet" href="/assets/css/styles.css")
    link(rel="shortcut icon" href="img/favicon.png")
    
  body
    include /includes/top-bar
    block main
    include /includes/footer
    script(src="/assets/js/scripts-min.js")
    script(async defer src="https://maps.googleapis.com/maps/api/js?key=AIzaSyCFJaROzUzJ-qXI_qbJoWj-jNscP-ytigQ&callback=initMap")
